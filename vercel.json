{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.4"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}