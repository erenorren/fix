{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.7.4"
    }
  ],
  "framework": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production"
  }
}
